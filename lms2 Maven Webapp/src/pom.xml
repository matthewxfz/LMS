  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  <name>lms Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <properties>
    <webVersion>3.1</webVersion>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies>
